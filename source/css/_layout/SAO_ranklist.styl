if hexo-config('SAO_ranklist.enable')
  @font-face
    font-family 'SAOUI'
    src url('https://img.lovelu.top/akilar-theme/ttf/SAOUI.ttf')
    font-display swap

  // #SAO-ranklist
  //   +minWidth2000()
  //     z-index 9999
  //     font-family 'SAOUI' ,Langar,-apple-system, sans-serif
  //     left calc(50% - 250px)
